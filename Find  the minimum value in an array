// Find the minimum value in an array
#include <stdio.h>

int main(){
    int arr[] = {12,15,22,26,30,18};
    int n = sizeof(arr);
    int min = arr[0];

    for (int i = 1; i < n; i++){
         if (arr[i] < min){
             min = arr[i];
         }
    }
    printf("minimum value in the array is: %d\n",min);
    return 0;
    }
